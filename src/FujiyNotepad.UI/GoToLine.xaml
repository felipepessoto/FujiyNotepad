<Window x:Class="FujiyNotepad.UI.GoToLine"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FujiyNotepad.UI"
        mc:Ignorable="d"
        Title="Go To Line" Height="130" Width="220" ResizeMode="NoResize">
    <Grid>
        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="Line number:" VerticalAlignment="Top" Margin="10,0,0,0"/>
        <TextBox Name="TxtLineNumber" HorizontalAlignment="Left" Height="23" Text="" VerticalAlignment="Top" Width="191" Margin="10,21,0,0"/>
        <Button Name="BtnGoTo" Content="Go To" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="10,49,0,0" Click="BtnGoTo_Click" IsDefault="True" />

    </Grid>
</Window>
